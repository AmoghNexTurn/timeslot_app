    "import datetime\n",
    "\n",
    "# Create the datetime.date object\n",
    "my_date = datetime.date(2025, 10, 10)\n",
    "\n",
    "# Convert the date object to the \"YYYYMMDD\" string format\n",
    "formatted_string = my_date.strftime(\"%Y%m%d\")\n",
    "\n",
    "# Print the result\n",
    "print(formatted_string)\n"
   ]
